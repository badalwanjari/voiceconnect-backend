{
    "version": 2,
    "name": "voiceconnect-backend",
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}